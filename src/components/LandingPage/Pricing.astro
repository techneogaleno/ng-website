<script>
  const plans = {
    mx: {
      free: "$ 00.0 MXN",
      standard: "$ 499 MXN*",
      premium: "$ 749 MXN*",
    },
    co: {
      free: "$ 0,00 COP",
      standard: "$ 105.000,00 COP*",
      premium: "$  158.000,00 COP*",
    },
    ve: {
      free: "$0.00 USD",
      standard: "$ 25 USD*",
      premium: "$ 38 USD*",
    },
  };
  /*
  const plans = {
    mx: {
      free: "$ 00.0 MXN",
      standard: "$ 1160 MXN*",
      premium: "$ 1950 MXN*",
    },
    co: {
      free: "$ 0,00 COP",
      standard: "$ 224.900 COP*",
      premium: "$ 454.900 COP*",
    },
    ve: {
      free: "$0.00 USD",
      standard: "$ 49 USD*",
      premium: "$ 99 USD*",
    },
  };
  */

  async function getCountry(): Promise<string> {
    const env = process.env.NODE_ENV;
    let country = "mx";

    if (env !== "development") {
      await fetch("https://api.ipify.org?format=json")
        .then((x) => x.json())
        .then(async ({ ip }) => {
          const response = await fetch(
            `https://api.ipgeolocation.io/ipgeo?apiKey=119d6b9c49c94a82958eadcb574a82a5&ip=${ip}`
          );
          const data = await response.json();
          country = data.country_code2.toLowerCase();
        });
    }
    return country;
  }

  async function getPrice(): Promise<any> {
    const country = await getCountry();
    if (country === "co") {
      return plans.co;
    } else if (country === "mx") {
      return plans.mx;
    } else {
      return plans.ve;
    }
  }

  const price = await getPrice();
  document.getElementsByClassName("free-plan")[0].innerHTML = price.free;
  document.getElementsByClassName("standard-plan")[0].innerHTML =
    price.standard;
  document.getElementsByClassName("premium-plan")[0].innerHTML = price.premium;
</script>

